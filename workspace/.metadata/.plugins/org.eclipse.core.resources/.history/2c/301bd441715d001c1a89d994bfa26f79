#include <iostream>

using namespace std;

class Solution {
public:
	bool findTriplets(int arr[], int n)
	{
			int i, j, k;

			for (i = 0; i < (n-2) ; i++) {
				for (j = (i + 1), j < (n - 1); j++) {
					for (k = (j + 1); k < n; k++) {
					}
				}
			}
	}
};


